<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/mainFragment">
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.speakwithai.basestructure.ui.explore.ExploreFragment"
        android:label="FirstFragment">
        <action
            android:id="@+id/action_exploreFragment_to_chatFragment2"
            app:destination="@id/chatFragment2" />
    </fragment>

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.speakwithai.basestructure.ui.mainFragment.MainFragment"
        android:label="SecondFragment">
        <action
            android:id="@+id/action_mainFragment_to_chatFragment2"
            app:destination="@id/chatFragment2" />
        <action
            android:id="@+id/action_mainFragment_to_premiumRequiredDialogFragment"
            app:destination="@id/premiumRequiredDialogFragment" />

    </fragment>

    <fragment
        android:id="@+id/moreFragment"
        android:name="com.speakwithai.basestructure.ui.more.MoreFragment"
        android:label="ThirdFragment">
        <action
            android:id="@+id/action_moreFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment" />
        <action
            android:id="@+id/action_moreFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
        <action
            android:id="@+id/action_moreFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment2"
        android:name="com.speakwithai.basestructure.ui.chat.ChatFragment"
        android:label="ChatFragment" >
        <action
            android:id="@+id/action_chatFragment2_to_mainFragment"
            app:destination="@id/mainFragment" />
        <action
            android:id="@+id/action_chatFragment2_to_premiumRequiredDialogFragment"
            app:destination="@id/premiumRequiredDialogFragment" />
    </fragment>

    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.speakwithai.basestructure.ui.privacypolicy.PrivacyPolicyFragment"
        android:label="fragment_privacy_policy"
        tools:layout="@layout/fragment_privacy_policy" >
        <argument
            android:name="isim"
            app:argType="string" />
    </fragment>
    <dialog
        android:id="@+id/premiumRequiredDialogFragment"
        android:name="com.speakwithai.basestructure.ui.PremiumRequiredDialogFragment.PremiumRequiredDialogFragment"
        android:label="PremiumRequiredDialogFragment" >
        <action
            android:id="@+id/action_premiumRequiredDialogFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </dialog>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.speakwithai.basestructure.ui.login.signIn.SignInFragment"
        android:label="SignInFragment" >
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_signInFragment_to_moreFragment"
            app:destination="@id/moreFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.speakwithai.basestructure.ui.login.singUp.SignUpFragment"
        android:label="SignUpFragment" >
        <action
            android:id="@+id/action_signUpFragment_to_moreFragment"
            app:destination="@id/moreFragment" />
    </fragment>
    <fragment
        android:id="@+id/pickUpFragment"
        android:name="com.speakwithai.basestructure.ui.pickUp.PickUpFragment"
        android:label="PickUpFragment" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.speakwithai.basestructure.ui.settings.SettingsFragment"
        android:label="SettingsFragment" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.speakwithai.basestructure.ui.profile.ProfileFragment"
        android:label="ProfileFragment" >
        <action
            android:id="@+id/action_profileFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/qrCreaterFragment"
        android:name="com.speakwithai.basestructure.ui.qrCreator.QrCreaterFragment"
        android:label="QrCreaterFragment" />
    <fragment
        android:id="@+id/qrReaderFragment"
        android:name="com.speakwithai.basestructure.qrRead.QrReaderFragment"
        android:label="QrReaderFragment" />
    <fragment
        android:id="@+id/cryptoNewsFragment"
        android:name="com.speakwithai.basestructure.ui.cryptoNews.CryptoNewsFragment"
        android:label="CryptoNewsFragment" >
        <action
            android:id="@+id/action_cryptoNewsFragment_to_newsDetailFragment"
            app:destination="@id/newsDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/newsDetailFragment"
        android:name="com.speakwithai.basestructure.ui.cryptoNews.newsDetail.NewsDetailFragment"
        android:label="NewsDetailFragment" />
    <fragment
        android:id="@+id/cryptoMainFragment"
        android:name="com.speakwithai.basestructure.ui.crypto.cryptoMain.CryptoMainFragment"
        android:label="CryptoMainFragment" />
    <fragment
        android:id="@+id/chatGptFragment"
        android:name="com.speakwithai.basestructure.ui.chatGpt.ChatGptFragment"
        android:label="ChatGptFragment" />
    <fragment
        android:id="@+id/metaFragment"
        android:name="com.speakwithai.basestructure.ui.metaAi.MetaFragment"
        android:label="MetaFragment" />
    <fragment
        android:id="@+id/wallpaperFragment"
        android:name="com.speakwithai.basestructure.ui.wallpaper.WallpaperFragment"
        android:label="WallpaperFragment" />
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.speakwithai.basestructure.ui.wallpaper.detail.DetailFragment"
        android:label="DetailFragment">
        <argument
            android:name="photo"
            app:argType="com.speakwithai.basestructure.data.model.response.wallpaper.Photo"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/wallpaperResultFragment"
        android:name="com.speakwithai.basestructure.ui.wallpaper.result.WallpaperResultFragment"
        android:label="WallpaperResultFragment">
        <argument
            android:name="url"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/musicFragment"
        android:name="com.speakwithai.basestructure.ui.music.MusicFragment"
        android:label="MusicFragment" />
    <fragment
        android:id="@+id/geminiFragment"
        android:name="com.speakwithai.basestructure.ui.gemini.GeminiFragment"
        android:label="GeminiFragment" />
    <fragment
        android:id="@+id/textToImageFragment"
        android:name="com.speakwithai.basestructure.ui.textToImage.TextToImageFragment"
        android:label="TextToImageFragment" />
</navigation>
